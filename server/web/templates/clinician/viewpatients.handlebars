<div class="container-fluid h-100">
    <div class="row h-100">
    {{>dashboardNav}}
      <div class="col-sm-9 col-md-10">
        <div style="padding: 30px; color: #525B74;">
          <br><br>
          <h1 style= "text-align: center; font-size: 50px; font-weight: bold;" >Hello {{formatName user.name}}, select a patient to continue.</h1>
          <br><br>
          <form style= "padding-left: 50px;">
            <div class="form-group">
              <div class="search">
                <span class="fa fa-search"></span>
                <label id="searchLabel" for="search">Search by Patient ID
                  <input type="search" class="form-control" id="search" onkeyup="filter()" placeholder:"Search by Patient ID">
                </label>
             </div>
           </div>
         </form>
         <br><br>    
         <ul id ="ul" class = "ul">
           {{#each users}}
             <li style= "padding: 10px">
               <div class="listButtons">
                 <a href= '/clinician/patientexercises/{{this}}' style="color: #525B74;">{{this}}</a>
              </div> 
            <li>
          {{/each}}
         </ul>
       </div>
    </div>
  </div>
</div>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<link rel="stylesheet" href="../public/css/styleSheet/viewPatients.css">
<script type="text/javascript" src="../../public/scripts/clinician.js"></script>
